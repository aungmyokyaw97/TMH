{
    "name": "aungmyokyaw/tmh",
    "description": "Laravel SMS package for Tah Moe Hnye`",
    "keywords": ["laravel","TMH","package","Tah Moe Hnye","SMS","OTP"],
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.0|^8.1|^8.2",
        "guzzlehttp/guzzle": "^6.3|^7.5",
        "laravel/framework": "^10.0|^9.0|^8.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Amk\\Tmh\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amk\\Tmh\\Provider\\TmhServiceProvider"
            ],
            "aliases": {
                "TMH" : "Amk\\Tmh\\Facades\\TMH"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amk\\Tmh\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "AungMyoKyaw",
            "email": "aungmyokyaw28637@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "orchestra/testbench": "^v7.0.0"
    }
}
